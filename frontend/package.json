{
  "name": "kokalee-frontend",
  "version": "0.0.0",
  "private": true,
  "description": "A Vue.js project",
  "author": "Kokalee",
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run clean && npm run compile",
    "ci:jenkins": "karma --env=jk start karma.coverage.js && npm run coverage:remap",
    "ci:teamcity": "karma --env=tc start karma.coverage.js && npm run coverage:remap",
    "clean": "rimraf dist && rimraf coverage",
    "compile": "webpack --config config/webpack.config.prod.js",
    "coverage": "npm run coverage:run && npm run coverage:remap && npm run coverage:open",
    "coverage:open": "opn coverage/html-ts/index.html",
    "coverage:remap": "remap-istanbul -i coverage/json/coverage-final.json -o coverage/html-ts -t html",
    "coverage:run": "cross-env NODE_ENV=development karma start karma.coverage.js",
    "dev": "run-p dev:*",
    "dev:hoodie": "hoodie --port 8084 --public dist",
    "dev:vue": "webpack-dev-server --hot --inline",
    "dev:vuetype": "vuetype --watch src/components",
    "git-pre-commit": "lint-staged",
    "postinstall": "cd ..; npm install",
    "lint": "tslint --project tsconfig.json --format stylish 'src/**/*.ts'",
    "serve": "http-server dist/ -g -o",
    "sort-package-json-and-add": "sort-package-json && git add package.json",
    "sort-package-json-if-fully-staged": "unset GIT_DIR; git -c core.pager= ls-files -m package.json --error-unmatch || npm run sort-package-json-and-add",
    "start": "hoodie --public dist",
    "test": "cross-env NODE_ENV=development karma start karma.unit.js",
    "test:debug": "cross-env NODE_ENV=development karma start karma.debug.js",
    "test:watch": "cross-env NODE_ENV=development karma start karma.unit.js --singleRun=false --auto-watch"
  },
  "dependencies": {
    "@hoodie/client": "^10.1.1",
    "axios": "~0.17.1",
    "bootstrap": "^4.0.0",
    "bootstrap-vue": "^2.0.0-rc.1",
    "hoodie": "^28.2.5",
    "jquery": "^3.3.1",
    "popper": "^1.0.1",
    "popper.js": "^1.13.0",
    "pouchdb": "^6.4.3",
    "ts-loader": "^3.4.0",
    "uiv": "^0.14.5",
    "vue": "^2.5.13",
    "vue-class-component": "^6.1.2",
    "vue-router": "~3.0.1",
    "vue-template-compiler": "^2.5.13"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.48",
    "@types/node": "^8.5.9",
    "@types/sinon": "~4.0.0",
    "@types/webpack-env": "^1.13.5",
    "autoprefixer": "^7.2.5",
    "chai": "~4.1.2",
    "compression-webpack-plugin": "~1.0.1",
    "copy-webpack-plugin": "~4.2.3",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "es6-promise": "~4.1.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^1.1.6",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "~0.10.0",
    "istanbul-instrumenter-loader": "~3.0.0",
    "json-loader": "~0.5.7",
    "karma": "~1.7.1",
    "karma-chai": "~0.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage": "~1.1.1",
    "karma-junit-reporter": "~1.2.0",
    "karma-mocha": "~1.3.0",
    "karma-mocha-reporter": "~2.2.5",
    "karma-sinon": "~1.0.5",
    "karma-source-map-support": "~1.2.0",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-teamcity-reporter": "~1.0.1",
    "karma-webpack": "^2.0.9",
    "lint-staged": "^5.0.0",
    "lodash.merge": "~4.6.0",
    "minimist": "~1.2.0",
    "mocha": "~4.0.1",
    "ncp": "~2.0.0",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "opn-cli": "~3.1.0",
    "postcss-loader": "^2.1.0",
    "purify-css": "^1.2.6",
    "purifycss-webpack": "^0.7.0",
    "raw-loader": "~0.5.1",
    "remap-istanbul": "^0.9.6",
    "rimraf": "~2.6.2",
    "sass-loader": "^6.0.6",
    "sinon": "^4.2.2",
    "sort-package-json": "^1.7.1",
    "style-loader": "^0.19.1",
    "tslint": "^5.9.1",
    "tslint-consistent-codestyle": "^1.11.0",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-loader": "~3.5.3",
    "typescript": "^2.7.1",
    "url-loader": "^0.6.2",
    "vue-hot-reload-api": "^2.2.4",
    "vue-loader": "^13.7.1",
    "vue-property-decorator": "^6.0.0",
    "vue-ts-loader": "0.0.3",
    "vuetype": "github:forivall/vuetype#vue-cc-6",
    "webpack": "~3.8.1",
    "webpack-dev-server": "^2.11.1"
  },
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "lint-staged": {
    "linters": {
      "package.json": [
        "npm run sort-package-json-if-fully-staged"
      ],
      "*.ts": [
        "tslint --project tsconfig.json --format stylish"
      ]
    }
  }
}
